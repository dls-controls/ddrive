
## Parameter SCT: Scan type
record(mbbi, "$(P)$(R)SCT_IN") {
    field(DESC, "Scan type")
    field(DTYP, "asynUInt32Digital")
    field(INP, "@asynMask($(PORT) $(ADDR) 0x0F $(TIMEOUT))DD_SCT")
    field(SCAN, "I/O Intr")
    
    field("ZRVL", "0")
    field("ONVL", "1")
    field("TWVL", "2")
    field("ZRST", "Scan off")
    field("ONST", "Sine scan")
    field("TWST", "Triangle scan")

}

record(mbbo, "$(P)$(R)SCT_OUT") {
    field(DESC, "Scan type")
    field(DTYP, "asynUInt32Digital")
    field(OUT, "@asynMask($(PORT) $(ADDR) 0x0F $(TIMEOUT))DD_SCT")
    field("ZRVL", "0")
    field("ONVL", "1")
    field("TWVL", "2")
    field("ZRST", "Scan off")
    field("ONST", "Sine scan")
    field("TWST", "Triangle scan")

}


